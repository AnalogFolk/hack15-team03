{
  "manifest_version": 2,

  "name": "Sample App",
  "description": "Sample App",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts":[{
    "js":["location.js"],
    "matches":["http://*/*"]
  }],
  "web_accessible_resources": ["app.js"]
}
